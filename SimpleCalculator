
public class SimpleCalculator {
	public static void main(String[] args) {
	
		double a = 23.5;
		double b = 4.8;
		
		System.out.println("a+b=" + (a+b));
		System.out.println("a-b=" + (a-b));
		System.out.println("a*b=" + (a*b));
		System.out.println("a/b=" + (a/b));
		System.out.println("A > B ? " + (a>b));
		System.out.println("A * B > 100 ? " + (a*b > 100));

	}

}
